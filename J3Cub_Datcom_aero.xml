<aerodynamics>

	<alphalimits unit="DEG">
		<min>  -4.0</min>
		<max>   14.0    </max>
	</alphalimits>

	<function name="fcs/diff-aileron-pos-deg">
		<description>
			Differential Aileron (degrees)
		</description>
		<difference>
			<property>fcs/right-aileron-pos-deg</property>
			<property>fcs/left-aileron-pos-deg</property>
		</difference>
	</function>
	
